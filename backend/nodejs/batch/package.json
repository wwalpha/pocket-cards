{
  "name": "batch",
  "version": "1.0.1",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "debug": "ts-node -r tsconfig-paths/register -r dotenv/config src/index.ts",
    "test": "jest",
    "test:local": "jest test/services/learningStatus.test.ts"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.0",
    "@types/lodash": "^4.14.180",
    "@types/node": "^17.0.22",
    "csv-parse": "^5.3.1",
    "dotenv": "^16.0.0",
    "express": "^4.18.2",
    "jest": "^29.2.2",
    "jest-extended": "^3.1.0",
    "nodemon": "^2.0.15",
    "ts-jest": "^29.0.3",
    "tsc-alias": "^1.6.4",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "@alphax/dynamodb": "^1.1.3",
    "aws-sdk": "^2.1097.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1"
  },
  "nodemonConfig": {
    "watch": [
      "src/**/*.ts"
    ],
    "delay": 3,
    "ignore": [
      "node_modules/*"
    ],
    "execMap": {
      "ts": "ts-node -r tsconfig-paths/register -r dotenv/config"
    },
    "events": {
      "crash": "sh -c 'lsof -i :${PORT:-8080} -t | xargs kill'"
    }
  }
}
